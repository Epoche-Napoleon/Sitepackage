   {
  "name": "epochenapoleon/sitepackage",
  "type": "typo3-cms-extension",
  "description": "Create the sitepackage for the website EPOCHE NAPOLEON",
  "homepage": "http://www.epoche-napoleon.net",
  "support": {
    "source": "https://github.com/Epoche-Napoleon/sitepackage",
    "issues": "https://github.com/Epoche-Napoleon/sitepackage/issues"
  },
  "keywords": [
    "Sitepackage", "TYPO3 CMS", "Configuration"
  ],
  "authors": [
    {
    "name": "EPOCHE NAPOLEON - Michael Gnessner",
    "email": "technik@epoche-napoleon.net",
    "role": "Developer",
    "homepage": "http://www.epoche-napoleon.net"
    }
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "typo3/cms-core": "11.5.0 - 11.5.99 || 12.4.0 - 12.4.99"
  },
  "replace": {
    "typo3-ter/sitepackage": "self.version"
  },
  "autoload": {
    "psr-4": {
      "epochenapoleon\\Sitepackage\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "sitepackage"
    }
  }
}
